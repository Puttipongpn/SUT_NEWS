                    <script>
                        // รายการทั้งหมด
                        const allItems = document.querySelectorAll('.nav-item');

                        // จำนวนรายการที่ต้องการแสดงเริ่มต้น
                        const initialItemsToShow = 3;

                        // รายการที่ควรแสดงเริ่มต้น
                        const itemsToShow = Array.from(allItems).slice(0, initialItemsToShow);

                        // รายการที่ยังไม่ได้แสดง
                        const itemsToHide = Array.from(allItems).slice(initialItemsToShow);

                        // ลิงค์ "ดูเพิ่มเติม"
                        const showMoreLink = document.getElementById('show-more-link');

                        // ฟังก์ชันสำหรับแสดงรายการที่ซ่อนไว้
                        function showHiddenItems() {
                            itemsToHide.forEach(item => {
                                item.classList.remove('hidden');
                            });
                            showMoreLink.style.display = 'none'; // ซ่อนลิงค์ "ดูเพิ่มเติม"
                        }

                        // เพิ่มการฟังก์ชันสำหรับการคลิกลิงค์ "ดูเพิ่มเติม"
                        showMoreLink.addEventListener('click', showHiddenItems);

