      position: relative;
      display: flex;
      justify-content: center;
      align-items: center;

    }

    .border:hover i {
      position: absolute;
      top: 10px;
    }

    .border i {
      position: absolute;
      top: -100px;
      right: 10px;
      font-size: 25px;
      color: white;
      transition: all 0.5s;
    }

    .border .i-red {
      color: red;
    }

    .border img {
      height: 100%;
      width: 100%;
      object-fit: cover;
      transition: all 2s;
    }

    .central-div {
      display: flex;
      justify-content: space-between;
      padding: 20px 10px;
    }

    .central-div .fa-bookmark {
