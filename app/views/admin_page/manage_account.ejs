<%- include('../header')  %>
    <div class="card-body">
        <div class="card-title">
            <span class="material-symbols-outlined material-icons">
                
            </span>
            
        </div>
        <a class="btn btn-info mt-3" href="/addUser">
            <span class="material-symbols-outlined material-icons">
                add
            </span>
            เพิ่มรายการ
        </a>
        <table class="table table-bordered table-striped mt-3">
            <thead>
                <tr>
                    <th>Name</th>
                    <th width="200px">Email</th>
                    <th width="200px">ROW</th>
                    <th width="100px">View</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach((users) => { %>
                <tr>
                    
                    <td><%= users.name %></td>
                    <td><%= users.email %></td>
                    <td><%= users.row %></td>
                    <td>
                        <a class="btn btn-info" href="/view_user/<%= users.id %>">
                            <span class="material-symbols-outlined material-icons">
                                View
                            </span>
                        </a>
                    </td>
                </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</div>